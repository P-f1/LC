{
	"name": "tibco-mr4r-videoservice",
	"title": "Video Stream Service Connection",
	"author": "TIBCO Software Inc.",
	"type": "flogo:connector",
	"version": "1.0.0",
	"display": {
		"description": "This is video service connector",
		"category": "MR4R_CV",
		"visible": true,
		"smallIcon": "videoservice.png"
	},
	"ref": "github.com/P-f1/LC/MR4R_CV/connector/sse",
	"keyfield": "name",
	"settings": [
		{
			"name": "name",
			"type": "string",
			"required": true,
			"display": {
			"name": "Connection Name",
			"description": "Name of the connection"
			}
		},
		{
			"name": "description",
			"type": "string",
			"display": {
				"name": "Description",
				"description": "Connection description"
			}
		},
		{
			"name": "port",
			"type": "string",
			"required": true,
			"display": {
				"name": "Server port",
				"description": "Video server port",
				"type": "string",
        			"visible": true
			}
		},
		{
			"name": "path",
			"type": "string",
			"required": false,
			"display": {
				"name": "Path",
				"description": "Path of the resource which is requested",
				"type": "string",
        			"visible": true
			}
		},
		{
			"name": "tlsEnabled",
			"type": "boolean",
			"display": {
				"name": "TLS enabled",
				"description": "TLS enabled"
			},
			"value": false,
			"required": true
		},
		{
			"name": "uploadCRT",
			"type": "boolean",
			"display": {
				"name": "Upload TLS certificate",
				"description": "Upload TLS certificate from local file"
			},
			"value": true,
			"required": false
		},
		{ 
			"name": "tlsCRT",
			"type": "string", 
			"display": { 
				"name": "TLS certificate",
				"description": "TLS certificate", 
				"type": "fileselector", 
				"fileExtensions": [".pem",".crt"] 
			} 
		},
		{ 
			"name": "tlsKey",
			"type": "string", 
			"display": { 
				"name": "TLS key",
				"description": "TLS key", 
				"type": "fileselector", 
				"fileExtensions": [".pem",".key"] 
			} 
		},
		{
			"name": "tlsCRTPath",
			"type": "string",
			"display": {
				"name": "TLS certificate location",
				"description": "TLS certificate location",
				"type": "string"
			}
		},
		{
			"name": "tlsKeyPath",
			"type": "string",
			"display": {
				"name": "TLS key location",
				"description": "TLS key location",
				"type": "string"
			}
		}
	],
	"actions": [
		{
			"name": "Connect",
			"display": {
				"readonly": true
			}
		}
	]
}
